{
  "LOG": true,
  "NON_INTERACTIVE_MODE": true,
  "API_TIMEOUT_MS": 600000,
  "Providers": [
    {
      "name": "openai-responses",
      "api_base_url": "https://api.openai.com/v1/responses",
      "api_key": "YOUR_API_KEY_HERE",
      "models": [
        "gpt-4o",
        "gpt-4o-mini",
        "o3",
        "o3-mini",
        "o3-mini-2025-01-31"
      ],
      "transformer": {
        "use": ["responses-api"]
      }
    },
    {
      "name": "openai-chat",
      "api_base_url": "https://api.openai.com/v1/chat/completions",
      "api_key": "YOUR_API_KEY_HERE",
      "models": [
        "gpt-3.5-turbo",
        "gpt-4-turbo"
      ],
      "transformer": {
        "use": ["openai-response-format"]
      }
    }
  ],
  "Router": {
    "default": "openai-chat,gpt-3.5-turbo",
    "structuredOutput": "openai-responses,o3-mini",
    "jsonMode": "openai-responses,gpt-4o",
    "think": "openai-responses,o3-mini",
    "reasoning": "openai-responses,o3",
    "longContext": "openai-responses,gpt-4o"
  }
}